Check whether a number is palindrom or not ?
// Not selected edge cases handling

isPalindrome(int num){
  boolean res = false;
  boolean neg = false;
  if(num < 0){
     neg = true;
     num = num * -1;
  }
  if(num%10 == 0){
     res = true;
  }
  int rem;
  int quo;
  int rev = 0;
  while(quo<0){
    rem = num % 10 ;
    quo = num / 10 ;
    rev = rev * 10 + rem
  }
  
  res = rev == num ? true : false;
  
  return res;
}
